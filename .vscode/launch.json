{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Dev (Default)",
      "runtimeExecutable": "mise",
      "runtimeArgs": [
        "x",
        "--",
        "pnpm",
        "run",
        "dev"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Dev Android",
      "runtimeExecutable": "mise",
      "runtimeArgs": [
        "r",
        "run:dev",
        "android"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Dev iOS",
      "runtimeExecutable": "mise",
      "runtimeArgs": [
        "r",
        "run:dev",
        "ios"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Prod Android",
      "runtimeExecutable": "mise",
      "runtimeArgs": [
        "r",
        "run:prod",
        "android"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Prod iOS",
      "runtimeExecutable": "mise",
      "runtimeArgs": [
        "r",
        "run:prod",
        "ios"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}